rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    // This rule allows any authenticated user to read and write to the entire
    // database. This is suitable for development and prototyping but should be
    // secured before moving to production.
    match /{document=**} {
      allow read, write: if request.auth != null;
    }
  }
}
